{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "vue"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ee449f",
    "activityBar.activeBorder": "#ee449f",
    "activityBar.background": "#3281c2",
    "activityBar.foreground": "#3281c2",
    "activityBar.inactiveForeground": "#ee449f",
    "activityBarBadge.background": "#3281c2",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#2D2D2D",
    "statusBar.foreground": "#3281c2",
    "statusBarItem.hoverBackground": "#3281c2",
    "titleBar.activeBackground": "#3281c2",
    "titleBar.activeForeground": "#ee449f",
    "titleBar.inactiveBackground": "#3281c2",
    "titleBar.inactiveForeground": "#3281c2"
  }
}
